LINKOBJ  = "player.o" "camera_controller.o" "scene.o" "main.o" "world.o" "entity.o" "resource_manager.o" "source_private.res"
CLEANOBJ = "player.o" "camera_controller.o" "scene.o" "main.o" "world.o" "entity.o" "resource_manager.o" "source_private.res" "source.exe"

.PHONY: all all-before all-after clean clean-custom

all: all-before $(BIN) all-after

clean: clean-custom
	-$(RM) $(CLEANOBJ) >NUL 2>&1

source.exe: $(OBJ)

	$(CXX) $(LINKOBJ) -o "source.exe" $(LIBS)

player.o: player.cpp constants.h player.h includes.h entity.h other.h
	$(CXX) -c "player.cpp" -o "player.o" $(CXXFLAGS)  -finput-charset=UTF-8 -fexec-charset=windows-1251

camera_controller.o: camera_controller.cpp camera_controller.h constants.h includes.h
	$(CXX) -c "camera_controller.cpp" -o "camera_controller.o" $(CXXFLAGS) 

scene.o: scene.cpp camera_controller.h constants.h world.h player.h resource_manager.h includes.h scene.h entity.h other.h
